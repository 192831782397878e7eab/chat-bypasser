local function transformText(inputText)
    -- Replace the specific letters with transformed versions
    local transformed = inputText:gsub("a", "а")
    transformed = transformed:gsub("u", "u〪")
    transformed = transformed:gsub("i", "i〪")
    transformed = transformed:gsub("n", "ⴖ")
    transformed = transformed:gsub("s", "ร")
    
    local words = {}
    for word in transformed:gmatch("%S+") do
        table.insert(words, word)
    end
    
    return table.concat(words, "")
end

